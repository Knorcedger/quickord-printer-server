{
  "name": "quickord-printer-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "jest": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand --coverage",
    "jest:no-coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand --collectCoverage=false",
    "jest:watch": "npm run jest -- --watchAll",
    "ci": "sh ./scripts/ci.sh",
    "start": "npm run start:test",
    "start:dev": "tsx watch src/index.ts --noupdate",
    "start:test": "tsx watch src/index.ts --noupdate",
    "start:ci": "node ./dist/index.js",
    "test": "npm run lint && npm run jest",
    "dev": "npm run start:test",
    "debug": "node --inspect=0.0.0.0:9229 -r ts-node/register src/index.ts --watch",
    "----------------------------------Typescript----------------------------------": "",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "npm run typecheck && sh ./scripts/build.sh",
    "build:code": "rm -rf dist && mkdir dist && npx tsc --outDir dist/src",
    "build:watch": "tsx watch ./src --exec 'npm run build'",
    "build:bundle": "esbuild ./dist/src/index.js --bundle --platform=node --target=node24.8.0 --outfile=./dist/bundle.cjs --external:serialport --external:@serialport/bindings* --external:*.node",
    "----------------------------------Build----------------------------------": "",
    "build:exe": "npm run build:bundle && nexe ./dist/bundle.cjs --target windows-x64-24.8.0 --build --external:serialport --external:@serialport/bindings* --external:*.node --output builds/builds/printerServer.exe",
    "copy-bindings": "cp -r node_modules/@serialport* builds/node_modules/@serialport"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@serialport/bindings-cpp": "^12.0.1",
    "@types/jszip": "^3.4.0",
    "adm-zip": "^0.5.15",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jszip": "^3.10.1",
    "nanoid": "^5.0.4",
    "nconf": "^0.12.1",
    "network": "^0.7.0",
    "node-fetch": "^3.3.2",
    "node-gyp-build": "^4.8.4",
    "node-thermal-printer": "^4.4.3",
    "ping": "^0.4.4",
    "serialport": "^12.0.0",
    "sharp": "^0.34.3",
    "signale": "^1.4.0",
    "transliteration": "^2.3.5",
    "unzipper": "^0.12.3",
    "yauzl-promise": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@octokit/rest": "^20.1.1",
    "@types/adm-zip": "^0.5.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/jest": "^29.5.10",
    "@types/nconf": "^0.10.6",
    "@types/ping": "^0.4.4",
    "@types/signale": "^1.4.7",
    "@types/unzipper": "^0.10.11",
    "@types/yauzl-promise": "^4.0.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.25.2",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^23.11.0"
  }
}
